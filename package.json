{
  "name": "express-skeleton-3",
  "version": "1.0.0",
  "description": "A Skeleton Application",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "npm run serve",
    "serve": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "lint": "npm run lint:quiet",
    "lint:quiet": "eslint --quiet './**/*.js'",
    "lint:all": "eslint './**/*.js'",
    "lint:fix": "eslint './**/*.js' --quiet --fix",
    "test:nocoverage": "ava",
    "test:withcoverage": "nyc ava",
    "test": "npm run test:withcoverage",
    "gendoc": "apidoc -i routes/ -o public/docs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "agenda": "^4.2.1",
    "axios": "^1.7.7",
    "bcrypt": "^5.0.1",
    "cheerio": "^1.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "email-templates": "^8.0.10",
    "express": "~4.16.1",
    "express-jwt": "^6.1.1",
    "googleapis": "^144.0.0",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "mammoth": "^1.8.0",
    "mongodb-memory-server": "^8.4.1",
    "mongoose": "^6.2.6",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^23.7.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "randomstring": "^1.2.2",
    "tesseract.js": "^5.1.1",
    "xlsx": "^0.18.5"
  },
  "eslintIgnore": [
    "public/",
    "bin/"
  ],
  "devDependencies": {
    "apidoc": "^0.17.7",
    "ava": "^4.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "faker": "^6.6.6",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:quiet"
    }
  }
}
